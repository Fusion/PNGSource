{
  "name": "mysite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "go run cmd/webview.go",
    "css": "postcss assets/css/source.css -o assets/css/style.css",
    "build-windows": "xgo --branch feature/packaging --go=1.17.2 --dest dist/windows --pkg pngsource --targets=windows/amd64 github.com/fusion/pngsource",
    "package-windows": "cp -r packaging/windows/* dist/windows/ && cd dist/windows && makensis pngsource.nsi",
    "target-windows": "yarn build-windows && yarn package-windows"
  },
  "dependencies": {
    "daisyui": "^1.16.6"
  },
  "devDependencies": {
    "cssnano": "^5.0.13",
    "postcss-resemble-image": "^2.1.1",
    "stylelint": "^14.1.0"
  }
}
